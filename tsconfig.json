{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@spaceduck/brand": ["packages/brand/src"],
      "@spaceduck/brand/*": ["packages/brand/src/*"],
      "@spaceduck/core": ["packages/core/src"],
      "@spaceduck/core/*": ["packages/core/src/*"],
      "@spaceduck/gateway": ["packages/gateway/src"],
      "@spaceduck/ui": ["packages/ui/src"],
      "@spaceduck/ui/*": ["packages/ui/src/*"],
      "@spaceduck/web": ["apps/web"],
      "@spaceduck/channel-whatsapp": ["packages/channels/whatsapp/src"],
      "@spaceduck/provider-bedrock": ["packages/providers/bedrock/src"],
      "@spaceduck/provider-gemini": ["packages/providers/gemini/src"],
      "@spaceduck/memory-sqlite": ["packages/memory/sqlite/src"],
      "@spaceduck/tool-web-search": ["packages/tools/web-search/src"],
      "@spaceduck/scheduler": ["packages/scheduler/src"]
    },
    "types": ["bun-types"],
    "jsx": "react-jsx"
  },
  "exclude": ["node_modules", "dist", "apps/spaceduck-website"]
}
